{
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["import", "node", "promise"],
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true,
    "node": true,
    "commonjs": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "ecmaFeatures": {
      "impliedStrict": true
    },
    "sourceType": "module"
  }
}
